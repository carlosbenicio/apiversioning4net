steps:
  - step: Checkout
    command: ''
  - step: Dependencies restore
    command:  dotnet restore ApiVersioning.sln
  - step: Build
    command:  dotnet build  ApiVersioning.sln
  - step: 'Pack'
    command: "dotnet pack /p:PackageVersion=${tag}"
  - step: 'Publish'
    command: "dotnet nuget src/bin/debug/CBJOne.Libraries.ApiVersioning.${tag}.nupkg -k oy2cq3j45brikznzdpjqgri3cmiidtlefjxjil2kww6jsi -s https://api.nuget.org/v3/index.json"
  